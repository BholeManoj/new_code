package Mock;

import java.util.ArrayList;

public class ArraySort {

	public static void main(String[] args) {
		int a [] = new int [5];
		 a  [0] = 1;
		 a  [1] = 0;
		 a  [2] = 6;
		 a  [3] = 0;
		 a  [4] = 2;
		 ArrayList ar= new ArrayList();
		// ar.add(0, ar);
		 for(int i = 0;i<a.length;i++)
		 {
			 if(a[i]==0)
			 {
				 ar.add(i, ar);
				 continue;
			 }
			 System.out.println(a[i]);
		 }
		 System.out.println(ar);
		
		 

	}

}
